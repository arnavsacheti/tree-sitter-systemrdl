{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "systemrdl",
      "camelcase": "SystemRDL",
      "title": "Systerm RDL 2.0",
      "scope": "source.systemrdl",
      "file-types": [
        "systemrdl",
        "rdl"
      ],
      "injection-regex": "^systemrdl$",
      "class-name": "TreeSitterSystemrdl"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "System RDL 2.0 Grammar for tree-sitter",
    "authors": [
      {
        "name": "Aliaksei Chapyzhenka"
      }
    ],
    "links": {
      "repository": "git+https://github.com/SystemRDL/tree-sitter-systemrdl.git"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}